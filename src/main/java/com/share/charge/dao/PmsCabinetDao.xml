<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.share.charge.dao.PmsCabinetDao">
    <resultMap id="BaseResultMap" type="com.share.charge.mybatis.generator.model.PmsCabinet">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="position" jdbcType="VARCHAR" property="position" />
        <result column="max_charge" jdbcType="INTEGER" property="maxCharge" />
    </resultMap>

    <sql id="Base_Column_List">
    id, position, max_charge
  </sql>

    <resultMap id="VoCabinetMesMap" type="com.share.charge.vo.VoCabinetMes">
        <id column="id" jdbcType="INTEGER" property="id" />
<!--        <result column="position" jdbcType="VARCHAR" property="position" />-->
<!--        <result column="max_charge" jdbcType="INTEGER" property="maxCharge" />-->
        <association property="chargeAvailable" column="id" javaType="integer"
                     select="com.share.charge.dao.PmsChargeDao.chargeInCabinet">

        </association>
    </resultMap>
    <select id="findByPosition" parameterType="String" resultMap="VoCabinetMesMap">
        select * from pms_cabinet where position like #{position}
    </select>



</mapper>